{
  "name": "fabricants",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "clean": "node_modules/.bin/rimraf dist/*",
    "copy-assets": "node_modules/.bin/ts-node tools/copyAssets",
    "lint": "node_modules/.bin/tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "node_modules/.bin/tsc",
    "build": "node_modules/.bin/npm-run-all clean lint tsc",
    "dev:start": "node_modules/.bin/npm-run-all build start",
    "dev": "node_modules/.bin/nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "express": "^4.17.1",
    "mongo": "^0.1.0",
    "mongoose": "^5.9.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/fs-extra": "^8.0.1",
    "@types/mongoose": "^5.7.1",
    "@types/shelljs": "^0.8.6",
    "fs-extra": "^8.1.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.3",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  }
}
